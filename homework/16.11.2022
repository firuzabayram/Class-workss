.card{
    border: 1px solid brown;
    width: 300px;
    height: 400px;
    text-align: center;
    border-radius: 5%;
}

.circle{
    width: 65px;
    height: 65px;
    background-color: rgb(163, 32, 82);
    margin: 30px auto;
    border-radius: 50%;
}

button{
background-color: brown;
color: aliceblue;
width: 79px;
height: 34px;
border: none;
}
-------------------------------------------------
.square {
  height: 500px;
  width: 500px;
  background-color: rgb(192, 27, 27);
  display: flex;
  justify-content: center;
  align-items: center;
}
.square1{
    height: 400px;
    width: 400px;
    background-color: aqua;
    display: flex;
    justify-content: center;
    align-items: center;
}
.square3{
    height: 300px;
    width: 300px;
    background-color: aqua;
    display: flex;
    justify-content: center;
    align-items: center;
}
.sqr4{
    height: 200px;
    width: 200px;
    background-color: rgb(255, 0, 170);
    display: flex;
    justify-content: center;
    align-items: center;
}
.sqr5{
    height: 100px;
    width: 100px;
    background-color: rgb(47, 0, 255);
    display: flex;
    justify-content: center;
    align-items: center;
}
.sqr6{
    height: 50px;
    width: 50px;
    background-color: rgb(77, 21, 60);
    display: flex;
    justify-content: center;
    align-items: center;
}
